    <section id="visit" class="py-5 bg-light">
        <div class="container">
            <h2 class="text-center display-3">Schedule Tour</h2>
            <hr class="section-hr mb-5">
            <div class="row">
                <div class="col-md-8 offset-md-2">
                    <form action="mailto:mattp@tbs.org" enctype="text/plain">
                        <div class="mb-3">
                          <label for="name" class="form-label">Name</label>
                          <div class="row">
                              <div class="col-md-6">
                                <input type="text" class="form-control" id="name" aria-describedby="name" placeholder="first" required>
                              </div>
                              <div class="col-md-6">
                                <input type="text" class="form-control" id="name" aria-describedby="name" placeholder="last" required>
                              </div>
                          </div>
                        </div>
                        <div class="mb-3">
                            <div class="row">
                                <div class="col-md-6">
                                    <label for="phone" class="form-label">Phone</label>
                                    <input type="tel" class="form-control" id="phone" placeholder="(925) 685-9169"> 
                                </div>
                                <div class="col-md-6">
                                    <label for="email" class="form-label">Email</label>
                                    <input type="email" class="form-control" id="email" placeholder="tbs@gmail.org"> 
                                </div>
                            </div>
                        </div>
                        <div class="mb-3">
                            <div class="row">
                                <div class="col-md-6">
                                    <label for="how-select" class="form-label">How Did You Hear About Us?</label>
                                    <select class="form-select" name="howHear" id="how-select">
                                        <option value="referral">Referral</option>
                                        <option value="flyer">Flyer</option>
                                        <option value="facebook">Facebook</option>
                                        <option value="other">Other</option>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <label for="refName" class="form-label">Referral Name</label>
                                    <input type="text" class="form-control" id="refName" placeholder="johnny appleseed"> 
                                </div>
                            </div>
                        </div>
                        <div id="gradeInterest" class="mb-3">
                            <label class="mb-2" for="gradesOfInterest">Grades of Interest (Check All That Apply)</label>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="tk">
                                        <label class="form-check-label" for="tk">Transitional Kindergarten</label>
                                    </div>
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="k">
                                        <label class="form-check-label" for="k">Kindergarten</label>
                                    </div>
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="1stGrade">
                                        <label class="form-check-label" for="1stGrade">1st Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="2ndGrade">
                                        <label class="form-check-label" for="2ndGrade">2nd Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="3rdGrade">
                                        <label class="form-check-label" for="3rdGrade">3rd Grade</label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="4thGrade">
                                        <label class="form-check-label" for="4thGrade">4th Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="5thGrade">
                                        <label class="form-check-label" for="5thGrade">5th Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="6thGrade">
                                        <label class="form-check-label" for="6thGrade">6th Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="7thGrade">
                                        <label class="form-check-label" for="7thGrade">7th Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="8thGrade">
                                        <label class="form-check-label" for="8thGrade">8th Grade</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="d-grid">
                            <button type="submit" class="btn btn-dark text-white">Submit</button>
                        </div>
                      </form>

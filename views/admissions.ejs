                            <div class="row">
                                <div class="col-md-6">
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="tk">
                                        <label class="form-check-label" for="tk">Transitional Kindergarten</label>
                                    </div>
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="k">
                                        <label class="form-check-label" for="k">Kindergarten</label>
                                    </div>
                                    <div class="mb-1 form-check">
                                        <input type="checkbox" class="form-check-input" id="1stGrade">
                                        <label class="form-check-label" for="1stGrade">1st Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="2ndGrade">
                                        <label class="form-check-label" for="2ndGrade">2nd Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="3rdGrade">
                                        <label class="form-check-label" for="3rdGrade">3rd Grade</label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="mb1 form-check">
                                        <input type="checkbox" class="form-check-input" id="4thGrade">
                                        <label class="form-check-label" for="4thGrade">4th Grade</label>
                                    </div>
                                    <div class="mb1 form-check">
